<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
 	 <bean id="theme.themeDAO"  class="fr.paris.lutece.plugins.theme.business.ThemeDAO" />
 	 <bean id="theme.themeService"  class="fr.paris.lutece.plugins.theme.service.ThemeService" />
	 <bean id="theme.sitePropertiesGroup" class="fr.paris.lutece.portal.service.site.properties.SitePropertiesGroup" >
        <property name="nameKey">
            <value>theme.site_properties.name</value>
        </property>
        <property name="descriptionKey">
            <value>theme.site_properties.description</value>
        </property>
        <property name="keysPrefix">
            <value>theme.site_property.</value>
        </property>
    </bean>
</beans>
 	